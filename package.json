{
  "name": "@codingame/monaco-vscode-api",
  "version": "0.0.0-semantic-release",
  "private": false,
  "description": "VSCode public API plugged on the monaco editor",
  "keywords": [],
  "author": {
    "name": "CodinGame",
    "url": "http://www.codingame.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CodinGame/monaco-vscode-api"
  },
  "type": "module",
  "main": "dist/api.js",
  "module": "dist/api.js",
  "exports": {
    ".": {
      "default": "./dist/api.js"
    },
    "./services": {
      "types": "./dist/services.d.ts",
      "default": "./dist/services.js"
    },
    "./service-override/messages": {
      "types": "./dist/messages.d.ts",
      "default": "./dist/messages.js"
    },
    "./service-override/notifications": {
      "types": "./dist/notifications.d.ts",
      "default": "./dist/notifications.js"
    },
    "./service-override/dialogs": {
      "types": "./dist/dialogs.d.ts",
      "default": "./dist/dialogs.js"
    },
    "./service-override/modelEditor": {
      "types": "./dist/modelEditor.d.ts",
      "default": "./dist/modelEditor.js"
    },
    "./service-override/configuration": {
      "types": "./dist/configuration.d.ts",
      "default": "./dist/configuration.js"
    },
    "./service-override/keybindings": {
      "types": "./dist/keybindings.d.ts",
      "default": "./dist/keybindings.js"
    },
    "./service-override/textmate": {
      "types": "./dist/textmate.d.ts",
      "default": "./dist/textmate.js"
    },
    "./service-override/languageConfiguration": {
      "types": "./dist/languageConfiguration.d.ts",
      "default": "./dist/languageConfiguration.js"
    },
    "./service-override/theme": {
      "types": "./dist/theme.d.ts",
      "default": "./dist/theme.js"
    },
    "./service-override/tokenClassification": {
      "types": "./dist/tokenClassification.d.ts",
      "default": "./dist/tokenClassification.js"
    },
    "./service-override/snippets": {
      "types": "./dist/snippets.d.ts",
      "default": "./dist/snippets.js"
    },
    "./service-override/languages": {
      "types": "./dist/languages.d.ts",
      "default": "./dist/languages.js"
    },
    "./monaco": {
      "types": "./dist/monaco.d.ts",
      "default": "./dist/monaco.js"
    },
    "./webpack-loader": {
      "types": "./dist/webpack-loader.d.ts",
      "default": "./dist/webpack-loader.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "services": [
        "./dist/services.d.ts"
      ],
      "service-override/messages": [
        "./dist/messages.d.ts"
      ],
      "service-override/notifications": [
        "./dist/notifications.d.ts"
      ],
      "service-override/dialogs": [
        "./dist/dialogs.d.ts"
      ],
      "service-override/modelEditor": [
        "./dist/modelEditor.d.ts"
      ],
      "service-override/configuration": [
        "./dist/configuration.d.ts"
      ],
      "service-override/keybindings": [
        "./dist/keybindings.d.ts"
      ],
      "service-override/textmate": [
        "./dist/textmate.d.ts"
      ],
      "service-override/languageConfiguration": [
        "./dist/languageConfiguration.d.ts"
      ],
      "service-override/theme": [
        "./dist/theme.d.ts"
      ],
      "service-override/tokenClassification": [
        "./dist/tokenClassification.d.ts"
      ],
      "service-override/snippets": [
        "./dist/snippets.d.ts"
      ],
      "service-override/languages": [
        "./dist/languages.d.ts"
      ],
      "monaco": [
        "./dist/monaco.d.ts"
      ],
      "webpack-loader": [
        "./dist/webpack-loader.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean:local && npm run compile:local && npm run lint && npm run generate-types && npm run compile-webpack-loader",
    "compile": "npm run compile:local --workspaces",
    "clean": "npm run clean:local --workspaces",
    "clean:local": "rm -rf dist/",
    "compile:local": "rollup --config rollup/rollup.config.ts --configPlugin 'typescript={tsconfig: `tsconfig.rollup.json`}' --vscode-version ${npm_package_config_vscode_version}",
    "compile-webpack-loader": "tsc --target es2020 -lib es2020 --module commonjs --skipLibCheck --moduleResolution node --declaration --outDir dist src/webpack-loader.ts && mv dist/webpack-loader.js dist/webpack-loader.cjs",
    "preprepare": "./scripts/install-vscode",
    "lint": "eslint {**/src/**/*.ts,./rollup/*.ts,./release.ts}",
    "generate-types": "tsc --project tsconfig.types.json && rollup --config rollup/rollup.types.config.ts --configPlugin 'typescript={tsconfig: `tsconfig.rollup.json`}' && rm -rf ./dist/types",
    "release": "node --loader ts-node/esm release.ts ${npm_package_config_vscode_version}",
    "start": "vite",
    "start:debug": "vite --debug --force"
  },
  "config": {
    "vscode": {
      "version": "1.69.0"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@codingame/eslint-config": "^1.1.6",
    "@codingame/tsconfig": "^1.1.1",
    "@octokit/rest": "^19.0.7",
    "@rollup/plugin-dynamic-import-vars": "^2.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/babel__core": "^7.20.0",
    "@types/throttle-debounce": "~5.0.0",
    "@types/semver": "^7.3.13",
    "@types/vscode": "~1.69.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "editorconfig": "~1.0.2",
    "eslint": "^8.35.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "js-cleanup": "^1.2.0",
    "recast": "^0.22.0",
    "rollup": "^3.17.3",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-styles": "^4.0.0",
    "semver": "^7.3.8",
    "throttle-debounce": "~5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "~4.1.4",
    "vscode-oniguruma": "~1.7.0",
    "webpack": "^5.75.0",
    "zx": "^7.2.0"
  },
  "dependencies": {
  },
  "peerDependencies": {
    "monaco-editor": "~0.34.0",
    "vscode-oniguruma": "^1.6.2",
    "vscode-textmate": "^7.0.1"
  },
  "overrides": {
    "rollup-plugin-styles": {
      "rollup": "3.17.3"
    }
  },
  "volta": {
    "node": "18.13.0",
    "npm": "9.4.1"
  }
}
